{
    "name": "genealabs/laravel-nova-categories",
    "description": "Drop-in Categories for your Laravel apps with a Nova admin dashboard.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "../laravel-overridable-model"
        }
    ],
    "require": {
        "genealabs/laravel-overridable-model": "*",
        "illuminate/support": "^10.0",
        "laravel/nova": "^4.0",
        "php": "^8.2",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelNovaCategories\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelNovaCategories\\Providers\\Service",
                "GeneaLabs\\LaravelNovaCategories\\Providers\\Nova"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
